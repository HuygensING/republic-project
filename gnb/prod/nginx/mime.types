# Add mimetypes according to file extension
# See also: www.nginx.com/resources/wiki/start/topics/examples/full

types {
  text/html                             html;
  text/css                              css;
  text/xml                              xml rss;
  image/gif                             gif;
  image/jpeg                            jpeg jpg;
  application/x-javascript              js;
  text/plain                            txt;
  image/png                             png;
}
